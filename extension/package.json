{
  "name": "memviz",
  "displayName": "memviz",
  "description": "Memory visualizer",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.93.0"
  },
  "categories": ["Other"],
  "activationEvents": ["onDebug"],
  "main": "./dist/extension.js",
  "contributes": {},
  "scripts": {
    "vscode:prepublish": "npm run build",
    "check:biome": "biome check --write",
    "check:tsc": "tsc",
    "check": "npm run check:biome && npm run check:tsc",
    "build": "npm run check && node esbuild.js --production",
    "package": "vsce package --no-dependencies",
    "watch": "node esbuild.js --watch",
    "watch:extension": "npm run watch"
  },
  "devDependencies": {
    "@types/node": "20.x",
    "@types/vscode": "^1.93.0",
    "@vscode/debugprotocol": "^1.67.0",
    "@vscode/vsce": "^3.1.0",
    "esbuild": "^0.23.1",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "process-def": "file:../process-def",
    "memviz-glue": "file:../memviz-glue"
  }
}
